    "require-dir": "^0.3.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "npm-run-all": "^1.4.0",
    "tsconfig-glob": "^0.3.3",
    "typescript": "^1.7.5"
